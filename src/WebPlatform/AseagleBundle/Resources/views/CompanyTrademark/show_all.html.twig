{% extends 'AseagleBundle::seller_layout.html.twig' %}

    {% block seller_content %}
        <!-- Portfolio Item Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Trademarks</h1>
            </div>
        </div>
        <!-- /.row -->
        {% for t in trademarks %}
            <!-- Portfolio Item Row -->
            <div class="row">
                <div class="col-md-6">
                    <h3>{{ t.name }}</h3>
                    <ul>
                        <li>Start Date: {{ t.startDate|date("d/m/Y") }}</li>
                        <li>End Date: {{ t.endDate|date("d/m/Y") }}</li>
                        <li>Registration Number: {{ t.registrationNumber }}</li>
                        <li>Approved Goods: {{ t.approvedGoods }}</li>
                    </ul>
                </div>
            </div>
            <hr>
        {% endfor %}

    {% endblock %}
