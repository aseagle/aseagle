{% extends 'AseagleBundle::seller_layout.html.twig' %}

    {% block seller_content %}
        <!-- Portfolio Item Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Patents</h1>
            </div>
        </div>
        <!-- /.row -->
        {% for p in patents %}
            <!-- Portfolio Item Row -->
            <div class="row">
                <div class="col-md-6">
                    <img class="img-responsive" width="500" height="300" src="{{ image_helper.generate_one_large_image_url(p.image) }}" alt="">
                </div>

                <div class="col-md-6">
                    <h3>{{ p.name }}</h3>
                    <ul>
                        <li>Country: {{ p.country.name }}</li>
                        <li>Products: {{ p.products }}</li>
                        <li>Annual Turnover: {{ p.annualTurnover }}</li>
                    </ul>
                </div>
            </div>
            <hr>
        {% endfor %}

    {% endblock %}
